    <profile id="7331-ee07-e3c9-6323" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" name="Artificer armour" hidden="false">
      <characteristics>
        <characteristic characteristicId="21befb24-fc85-4f52-a745-64b2e48f8228" name="Description" value="Artificer armour confers a 2+ Armour Save."/>
      </characteristics>
      <modifiers/>
    </profile>
    <profile id="afb2-6ed0-b7b0-3bd7" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" name="Broad Spectrum Data-Tether" hidden="false" book="Codex: Skitarii" page="77">
      <characteristics>
        <characteristic characteristicId="21befb24-fc85-4f52-a745-64b2e48f8228" name="Description" value="All friendly Skitarii units within 6&quot; gain +1 Leadership when affected by a Doctrina Imperative.  Does not stack with itself or enhanced data-tether."/>
      </characteristics>
